{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "resourceBaseName": {
            "value": "agent-${{RESOURCE_SUFFIX}}-${{TEAMSFX_ENV}}"
        },
        "botEntraAppClientId": {
            "value": "${{BOT_ID}}"
        },
        "botEntraAppClientSecret": {
            "value": "${{SECRET_BOT_PASSWORD}}"
        },
        "webAppSKU": {
            "value": "B1"
        },
        "botDisplayName": {
            "value": "${{APP_DISPLAY_NAME}}"
        },
        "modelDeploymentName": {
            "value": "${{APP_INTERNAL_NAME}}"
        },
        "modelName": {
            "value": "${{MODEL_NAME}}"
        },
        "modelVersion": {
            "value": "${{MODEL_VERSION}}"
        },
        "azureContentSafetySku": {
            "value": "S0"
        }
    }
}